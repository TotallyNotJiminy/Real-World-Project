<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_foodrate"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="fappnation.myfoodapplication.Foodrate">


    <ImageButton
        android:id="@+id/star2"
        android:layout_width="40dp"
        app:srcCompat="@drawable/starwhite"
        android:adjustViewBounds="false"
        android:padding="1dp"
        android:scaleType="fitStart"
        android:visibility="visible"
        android:backgroundTint="@android:drawable/screen_background_light_transparent"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/star1"
        android:layout_toEndOf="@+id/star1" />

    <ImageButton
        android:id="@+id/star5"
        app:srcCompat="@drawable/starwhite"
        android:adjustViewBounds="true"
        android:padding="1dp"
        android:scaleType="fitStart"
        android:visibility="visible"
        android:backgroundTint="@android:drawable/screen_background_light_transparent"
        android:layout_width="40dp"
        android:layout_height="40dp"

        android:layout_alignTop="@+id/star2"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:id="@+id/star4"
        app:srcCompat="@drawable/starwhite"
        android:adjustViewBounds="true"
        android:padding="1dp"
        android:scaleType="fitStart"
        android:visibility="visible"
        android:backgroundTint="@android:drawable/screen_background_light_transparent"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/star5"
        android:layout_toEndOf="@+id/star5" />

    <ImageButton
        android:id="@+id/star3"
        app:srcCompat="@drawable/starwhite"
        android:adjustViewBounds="true"
        android:padding="1dp"
        android:scaleType="fitStart"
        android:visibility="visible"
        android:backgroundTint="@android:drawable/screen_background_light_transparent"
        android:layout_width="40dp"
        android:layout_height="40dp"

        android:layout_alignTop="@+id/star4"
        android:layout_toEndOf="@+id/star4" />

    <ImageButton
        android:id="@+id/star1"
        android:layout_width="40dp"
        app:srcCompat="@drawable/starwhite"
        android:adjustViewBounds="true"
        android:padding="1dp"
        android:scaleType="fitStart"
        android:visibility="visible"
        android:backgroundTint="@android:drawable/screen_background_light_transparent"
        android:layout_height="40dp"
        android:layout_marginBottom="22dp"
        android:layout_above="@+id/Next1"
        android:layout_alignStart="@+id/Next1" />

    <Button
        android:text="Next"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="22dp"
        android:id="@+id/Next1"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/star3"
        android:background="@android:color/holo_green_light" />

    <TextView
        android:text="2.Cream"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:id="@+id/textView13"
        android:layout_below="@+id/textView12"
        android:layout_toEndOf="@+id/star4"
        android:textColor="@android:color/holo_blue_light"
        android:textStyle="normal|italic" />

    <TextView
        android:text="4.Carrots"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:id="@+id/textView20"
        android:layout_below="@+id/textView19"
        android:layout_alignStart="@+id/textView19"
        android:textColor="@android:color/holo_blue_light"
        android:textStyle="normal|italic" />

    <TextView
        android:text="1.Potato"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:layout_marginTop="21dp"
        android:layout_below="@+id/textView21"
        android:layout_alignStart="@+id/textView13"
        android:textColor="@android:color/holo_blue_light"
        android:textStyle="normal|italic" />

    <TextView
        android:text="3.Beans"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView19"
        android:layout_marginTop="11dp"
        android:textColor="@android:color/holo_blue_light"
        android:textStyle="normal|italic"
        android:layout_below="@+id/textView13"
        android:layout_alignStart="@+id/textView13" />

    <TextView
        android:text="Description"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:id="@+id/textView30"
        android:layout_below="@+id/imageView"
        android:layout_alignParentStart="true"
        android:textColor="@android:color/holo_blue_dark" />

    <TextView
        android:text="A baked potato garnished with creamy mashed potato ,beans and covered in carrot strips"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView30"
        android:layout_alignParentStart="true"
        android:layout_marginTop="13dp"
        android:id="@+id/textView33"
        android:textSize="14sp"
        android:textColor="@android:color/holo_blue_light" />

    <TextView
        android:text="Ingredients"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView21"
        android:layout_marginTop="85dp"
        android:textColor="@android:color/holo_blue_dark"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView12" />

    <ImageView
        android:layout_width="300dp"
        app:srcCompat="@drawable/bakedpotato"
        android:id="@+id/imageView"
        android:layout_height="200dp"
        android:layout_marginTop="69dp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/star4" />

    <TextView
        android:text="Jacked Potato"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView34"
        android:textSize="18sp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/textView21"
        android:layout_marginEnd="13dp"
        android:textColor="@android:color/holo_blue_dark" />

</RelativeLayout>